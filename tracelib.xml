<tracelibConfiguration>
<process>
    <name>sampleapp.exe</name>
    <output type="stdout"/>
    <serializer type="plaintext">
        <option name="timestamps">yes</option>
    </serializer>
    <tracepointset variables="yes">
        <verbosityfilter maxVerbosity="1"/>
    </tracepointset>
    <tracepointset  variables="yes">
        <verbosityfilter maxVerbosity="2"/>
    </tracepointset>
</process>
<process>
    <name>tracegen.exe</name>
    <output type="tcp">
        <option name="host">127.0.0.1</option>
        <option name="port">44123</option>
    </output>
    <serializer type="csv"/>
    <tracepointset variables="yes">
        <verbosityfilter maxVerbosity="1"/>
    </tracepointset>
</process>
</tracelibConfiguration>


