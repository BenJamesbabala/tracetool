<tracelibConfiguration>
<process>
    <name>tracegen.exe</name>
    <output type="tcp">
        <option name="host">127.0.0.1</option>
        <option name="port">44123</option>
    </output>
    <serializer type="csv"/>
    <tracepointset variables="yes">
        <verbosityfilter maxVerbosity="1"/>
    </tracepointset>
</process>
</tracelibConfiguration>


