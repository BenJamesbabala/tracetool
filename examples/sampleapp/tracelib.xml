<tracelibConfiguration>
<process>
    <name>sampleapp.exe</name>
    <output type="stdout"/>
    <serializer type="xml">
        <option name="beautifiedOutput">yes</option>
    </serializer>
    <tracepointset variables="yes">
        <verbosityfilter maxVerbosity="1"/>
    </tracepointset>
    <tracepointset  variables="yes">
        <verbosityfilter maxVerbosity="2"/>
    </tracepointset>
</process>
</tracelibConfiguration>


